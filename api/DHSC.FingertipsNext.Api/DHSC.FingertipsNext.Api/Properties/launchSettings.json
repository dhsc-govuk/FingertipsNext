{
  "$schema": "https://json.schemastore.org/launchsettings.json",
  "profiles": {
    "http": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": true,
      "applicationUrl": "http://localhost:5144",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "DB_SERVER": "localhost",
        "DB_NAME": "fingertips-db",
        "DB_USER": "<username>",
        "DB_PASSWORD": "<password>"
      }
    },
    "https": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": false,
      "applicationUrl": "https://localhost:7130;http://localhost:5144",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "DB_SERVER": "localhost",
        "DB_NAME": "fingertips-db",
        "DB_USER": "<username>",
        "DB_PASSWORD": "<password>"
      }
    }
  }
}
