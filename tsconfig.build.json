{
  "compilerOptions": {
    "module": "ES2022",
    "target": "es2022",
    "sourceMap": true,
    "strict": true,
    "declaration": true,
    "declarationMap": true,
    "noEmitOnError": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "jsx": "react",
    "noEmit": false
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}
