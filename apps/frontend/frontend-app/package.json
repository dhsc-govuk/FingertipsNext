{
  "name": "@fingertips/frontend-app",
  "version": "0.1.0",
  "private": true,
  "description": "The Fingertips frontend. A [Next.js](https://nextjs.org) project.",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "analyse": "cross-env ANALYZE=true npm run build",
    "dev:local-api": "concurrently \"npm run api:start\" \"npm run dev-no-mocks\" && npm run api:stop",
    "dev": "cross-env MOCK_SERVER=true npm run dev-no-mocks",
    "dev-no-mocks": "cross-env NODE_OPTIONS='--max-http-header-size=32768' next dev",
    "generate:api-client": "openapi-generator-cli generate",
    "generate:ft-mocks": "npx msw-auto-mock ./../../api/definition/swagger.yaml -o ./mock/server",
    "build": "next build",
    "start": "cross-env NODE_OPTIONS='--max-http-header-size=32768' next start",
    "start-local-mocks": "cross-env MOCK_SERVER=true npm run start",
    "api:start": "cross-env COMPOSE_BAKE=true docker compose --profile api up --build",
    "api:stop": "docker compose --profile api down",
    "start:all": "cross-env COMPOSE_BAKE=false docker compose --profile all up --build --remove-orphans --wait -d",
    "stop:all": "docker compose --profile all down",
    "lint": "eslint",
    "typecheck": "tsc",
    "test": "jest",
    "test-ci": "jest --ci --silent",
    "test-ui-local-mocks": "playwright test isolated_ui_tests --ignore-snapshots",
    "test-e2e-local-docker": "cross-env MOCK_SERVER=false npm run start:all && playwright test fully_integrated_e2e_tests || true && npm run stop:all",
    "test-e2e-local-update-snapshots": "cross-env MOCK_SERVER=false npm run start:all && npm run test-e2e-local-update-snapshots-stack-running",
    "test-e2e-local-update-snapshots-stack-running": "playwright test fully_integrated_e2e_tests --update-snapshots",
    "test-e2e-ci": "MOCK_SERVER=false playwright test fully_integrated_e2e_tests",
    "test-e2e-ci-azure": "MOCK_SERVER=false playwright test fully_integrated_e2e_tests --ignore-snapshots",
    "test-e2e-ui": "MOCK_SERVER=false playwright test fully_integrated_e2e_tests --ui --ignore-snapshots",
    "prettier": "prettier --write .",
    "prettier-ci": "prettier . --check"
  },
  "dependencies": {
    "@azure/monitor-opentelemetry": "^1.8.1",
    "@azure/search-documents": "^12.1.0",
    "@emotion/is-prop-valid": "^1.3.1",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-jaeger": "^1.30.0",
    "@opentelemetry/instrumentation-undici": "^0.10.0",
    "@opentelemetry/resources": "^1.30.0",
    "@opentelemetry/sdk-metrics": "^1.30.0",
    "@opentelemetry/sdk-node": "^0.57.0",
    "@opentelemetry/sdk-trace-base": "^1.30.0",
    "@opentelemetry/semantic-conventions": "^1.28.0",
    "@tanstack/react-query": "^5.74.4",
    "@turf/union": "^7.2.0",
    "@types/styled-components": "^5.1.34",
    "govuk-react": "^0.10.7",
    "highcharts": "^12.1.2",
    "highcharts-react-official": "^3.2.1",
    "next": "^15.3.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.1",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.17.0",
    "@estruyf/github-actions-reporter": "^1.10.0",
    "@faker-js/faker": "^9.3.0",
    "@next/bundle-analyzer": "^15.2.4",
    "@openapitools/openapi-generator-cli": "^2.19.1",
    "@playwright/test": "^1.52.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.2",
    "@types/testing-library__jest-dom": "^5.14.9",
    "axe-playwright": "^2.1.0",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.19.0",
    "eslint-config-next": "^15.1.6",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-playwright": "^2.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-mock-extended": "^4.0.0-beta1",
    "jest-styled-components": "^7.2.0",
    "msw": "^2.6.8",
    "playwright-ctrf-json-reporter": "^0.0.19",
    "playwright-webkit": "^1.52.0",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "zod": "^3.24.1"
  },
  "engines": {
    "npm": ">=10.9.0"
  },
  "overrides": {
    "axios": ">=1.8.2",
    "govuk-react": {
      "styled-components": "^5.3.0"
    },
    "whatwg-url": "^14.0.0"
  }
}
