{
  "name": "fingertips-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run api:start\" \"npm run dev:standalone\" && npm run api:stop",
    "dev:standalone": "next dev",
    "build": "next build",
    "start": "next start",
    "api:start": "docker compose --profile api up --build",
    "api:stop": "docker compose --profile api down",
    "lint": "next lint",
    "typecheck": "tsc",
    "test": "jest",
    "test-ci": "jest --ci --silent",
    "test-e2e": "cypress open",
    "prettier": "prettier --write .",
    "prettier-ci": "prettier ."
  },
  "dependencies": {
    "@types/styled-components": "^5.1.34",
    "govuk-react": "^0.10.7",
    "next": "^15.0.3",
    "react": "19.0.0-rc-66855b96-20241106",
    "react-dom": "19.0.0-rc-66855b96-20241106",
    "styled-components": "^6.1.13"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.13.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/testing-library__jest-dom": "^5.14.9",
    "concurrently": "^9.1.0",
    "cypress": "^13.16.0",
    "eslint": "^9.16.0",
    "eslint-config-next": "^15.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-mock-extended": "^4.0.0-beta1",
    "jest-styled-components": "^7.2.0",
    "postcss": "^8.4.49",
    "prettier": "^3.4.1",
    "tailwindcss": "^3.4.15",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "wait-on": "^8.0.1",
    "zod": "^3.23.8"
  },
  "overrides": {
    "govuk-react": {
      "react": "$react",
      "react-dom": "$react-dom"
    },
    "styled-components": {
      "react": "$react",
      "react-dom": "$react-dom"
    },
    "@testing-library/react": {
      "react": "$react",
      "react-dom": "$react-dom"
    }
  }
}
