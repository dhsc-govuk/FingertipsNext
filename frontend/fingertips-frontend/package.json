{
  "name": "fingertips-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:local-api": "concurrently \"npm run api:start\" \"next dev\" && npm run api:stop",
    "dev": "cross-env MOCK_SERVER=true next dev",
    "generate:api-client": "openapi-generator-cli generate",
    "generate:mocks": "npx msw-auto-mock ./../../api/definition/weather.json -o ./mock/server",
    "generate:ft-mocks": "npx msw-auto-mock ./../../api/definition/swagger.yaml -o ./mock/server",
    "build": "next build",
    "start": "next start",
    "api:start": "docker compose --profile api up --build",
    "api:stop": "docker compose --profile api down",
    "lint": "next lint",
    "typecheck": "tsc",
    "test": "jest",
    "test-ci": "jest --ci --silent",
    "test-e2e": "playwright test",
    "prettier": "prettier --write .",
    "prettier-ci": "prettier . --check"
  },
  "dependencies": {
    "@azure/search-documents": "^12.1.0",
    "@azure/monitor-opentelemetry": "^1.8.0",
    "@emotion/is-prop-valid": "^1.3.1",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/resources": "^1.30.0",
    "@opentelemetry/sdk-metrics": "^1.30.0",
    "@opentelemetry/sdk-node": "^0.57.0",
    "@opentelemetry/sdk-trace-base": "^1.30.0",
    "@opentelemetry/semantic-conventions": "^1.28.0",
    "@types/styled-components": "^5.1.34",
    "govuk-react": "^0.10.7",
    "highcharts": "^12.1.2",
    "highcharts-react-official": "^3.2.1",
    "next": "^15.1.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.1",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.17.0",
    "@faker-js/faker": "^9.3.0",
    "@openapitools/openapi-generator-cli": "^2.15.3",
    "@playwright/test": "^1.49.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.2",
    "@types/testing-library__jest-dom": "^5.14.9",
    "axe-playwright": "^2.0.3",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.17.0",
    "eslint-config-next": "^15.1.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-mock-extended": "^4.0.0-beta1",
    "jest-styled-components": "^7.2.0",
    "msw": "^2.6.8",
    "playwright-webkit": "^1.49.1",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "zod": "^3.24.1"
  },
  "engines": {
    "npm": ">=10.9.0"
  },
  "overrides": {
    "govuk-react": {
      "styled-components": "^5.3.0"
    },
    "whatwg-url": "^14.0.0"
  }
}
