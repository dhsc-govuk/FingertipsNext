{
  "name": "fingertips-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyse": "cross-env ANALYZE=true npm run build",
    "dev:local-api": "concurrently \"npm run api:start\" \"npm run dev-no-mocks\" && npm run api:stop",
    "dev": "cross-env MOCK_SERVER=true npm run dev-no-mocks",
    "dev-no-mocks": "cross-env NODE_OPTIONS='--max-http-header-size=32768' next dev",
    "bundle-swagger": "npx redocly bundle ./../../api/definition/swagger.yaml -o ./../../api/definition/fingertipsAPI.yaml",
    "generate:api-client": "npm run bundle-swagger && openapi-generator-cli generate",
    "generate:ft-mocks": "npm run bundle-swagger && npx msw-auto-mock ./../../api/definition/fingertipsAPI.yaml -o ./mock/server",
    "build": "next build",
    "start": "cross-env NODE_OPTIONS='--max-http-header-size=32768' next start",
    "start-local-mocks": "cross-env MOCK_SERVER=true npm run start",
    "api:start": "cross-env COMPOSE_BAKE=true docker compose --profile api up --build",
    "api:stop": "docker compose --profile api down",
    "start:all": "cross-env COMPOSE_BAKE=false docker compose --profile all up --build --remove-orphans --wait -d",
    "start:backend-services": "cross-env COMPOSE_BAKE=false docker compose --profile backend-services up --build --remove-orphans -d",
    "start:app-services": "cross-env COMPOSE_BAKE=false docker compose up api frontend --no-deps --build --remove-orphans --wait -d",
    "stop:all": "docker compose --profile all down",
    "stop:backend-services": "docker compose --profile backend-services down",
    "stop:app-services": "docker compose down api frontend",
    "lint": "eslint",
    "lint-all": "npm run prettier && npm run lint && npm run typecheck",
    "typecheck": "tsc",
    "test": "vitest",
    "test-ci": "vitest --silent",
    "open-test-coverage": "open coverage/lcov-report/index.html",
    "test-ui-local-mocks": "cross-env NODE_ENV=production AUTH_USE_PASSWORD_MOCK=true AUTH_TRUST_HOST=true playwright test isolated_ui_tests --ignore-snapshots",
    "test-e2e-local-docker": "cross-env MOCK_SERVER=false CHECK_TRENDS_ON_RESULTS_PAGE=false npm run start:all && playwright test fully_integrated_e2e_tests --ignore-snapshots --grep @ci || true && npm run stop:all",
    "test-e2e-ci": "MOCK_SERVER=false NODE_TLS_REJECT_UNAUTHORIZED=0 CHECK_TRENDS_ON_RESULTS_PAGE=false playwright test fully_integrated_e2e_tests --grep @ci --project=chromium",
    "test-e2e-cd": "MOCK_SERVER=false CHECK_TRENDS_ON_RESULTS_PAGE=true playwright test fully_integrated_e2e_tests ai_search_tests --grep @cd --project=chromium --ignore-snapshots",
    "test-e2e-ui-mode": "cross-env MOCK_SERVER=false CHECK_TRENDS_ON_RESULTS_PAGE=false playwright test fully_integrated_e2e_tests --ui --ignore-snapshots --grep @ci",
    "prettier": "prettier --write .",
    "prettier-ci": "prettier . --check"
  },
  "dependencies": {
    "@azure/monitor-opentelemetry": "^1.8.1",
    "@azure/search-documents": "^12.1.0",
    "@date-fns/utc": "^2.1.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-jaeger": "^1.30.0",
    "@opentelemetry/instrumentation-undici": "^0.10.0",
    "@opentelemetry/resources": "^1.30.0",
    "@opentelemetry/sdk-metrics": "^1.30.0",
    "@opentelemetry/sdk-node": "^0.57.0",
    "@opentelemetry/sdk-trace-base": "^1.30.0",
    "@opentelemetry/semantic-conventions": "^1.28.0",
    "@tanstack/react-query": "^5.81.2",
    "@tanstack/react-query-devtools": "^5.81.2",
    "@turf/union": "^7.2.0",
    "date-fns": "^4.1.0",
    "govuk-react": "^0.10.7",
    "highcharts": "^12.2.0",
    "highcharts-react-official": "^3.2.1",
    "html2canvas": "^1.4.1",
    "next": "^15.3.5",
    "next-auth": "^5.0.0-beta.28",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-paginate": "^8.3.0",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.17.0",
    "@estruyf/github-actions-reporter": "^1.10.0",
    "@faker-js/faker": "^9.3.0",
    "@next/bundle-analyzer": "^15.2.4",
    "@openapitools/openapi-generator-cli": "^2.19.1",
    "@playwright/test": "^1.53.0",
    "@redocly/cli": "^1.34.3",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^22.10.2",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/styled-components": "^5.1.34",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@vitejs/plugin-react": "^4.5.2",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/eslint-plugin": "^1.2.7",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.19.0",
    "eslint-config-next": "^15.1.6",
    "eslint-plugin-playwright": "^2.2.0",
    "fast-xml-parser": "^5.2.5",
    "jsdom": "^26.1.0",
    "msw": "^2.6.8",
    "playwright": "^1.53.2",
    "playwright-ctrf-json-reporter": "^0.0.21",
    "playwright-webkit": "^1.53.2",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "vitest-mock-extended": "^3.1.0",
    "zod": "^3.24.1"
  },
  "engines": {
    "npm": ">=10.9.0"
  },
  "overrides": {
    "axios": ">=1.8.2",
    "govuk-react": {
      "styled-components": "^5.3.0"
    },
    "whatwg-url": "^14.0.0"
  }
}
