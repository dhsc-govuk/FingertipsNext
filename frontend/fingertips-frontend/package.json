{
  "name": "fingertips-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run api:start\" \"npm run dev:standalone\" && npm run api:stop",
    "dev:standalone": "next dev",
    "build": "next build",
    "start": "next start",
    "api:start": "docker compose --profile api up --build",
    "api:stop": "docker compose --profile api down",
    "lint": "next lint",
    "typecheck": "tsc",
    "test": "jest"
  },
  "dependencies": {
    "@types/styled-components": "^5.1.34",
    "govuk-react": "^0.10.7",
    "next": "^15.0.3",
    "react": "19.0.0-rc-66855b96-20241106",
    "react-dom": "19.0.0-rc-66855b96-20241106",
    "styled-components": "^6.1.13"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.13.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "concurrently": "^9.1.0",
    "eslint": "^9.13.0",
    "eslint-config-next": "^15.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
